{
    "model_type": "lm",
    "sample_size": 153600,
    "sample_rate": 24000,
    "audio_channels": 1,
    "model": {
        "pretransform": {
            "type": "vocos_pretrained",
            "config": {
                "model_type": "charactr/vocos-encodec-24khz",
                "bandwidth": 3.0
            }
        },
        "conditioning": {
            "configs": [
                {
                    "id": "prompt",
                    "type": "phoneme",
                    "config": {
                        "max_length": 64,
                        "project_out": true
                    }
                }
            ],
            "cond_dim": 1536
        },
        "lm": {
            "codebook_pattern": "delay",
            "type": "mamba",
            "config": {
                "embed_dim": 1536,
                "n_layer": 16,
                "fused_add_norm": true,
                "residual_in_fp32": true,
                "rms_norm": true,
                "ssm_cfg": {
                    "d_state": 1
                }
            }
        }
    },
    "training": {
        "learning_rate": 8e-4,
        "demo": {
            "demo_every": 2500,
            "demo_cond": [
                {"prompt": "Once upon a time."},
                {"prompt": "Each conditioner takes in the relevant conditioning data."}
            ],
            "demo_cfg_scales": [1, 3]
        },
        "optimizer_configs": {
            "lm": {
                "optimizer": {
                    "type": "AdamW",
                    "config": {
                        "lr": 8e-4,
                        "betas": [0.8, 0.95],
                        "weight_decay": 0.1
                    }
                },
                "scheduler": {
                    "type": "OneCycleLR",
                    "config": {
                        "max_lr": 8e-4,
                        "epochs": 20,
                        "steps_per_epoch": 5750,
                        "pct_start": 0.05,
                        "cycle_momentum": false
                    }
                }
            }
        }
    }
}