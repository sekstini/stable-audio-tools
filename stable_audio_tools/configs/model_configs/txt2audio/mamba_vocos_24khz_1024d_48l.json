{
    "model_type": "lm",
    "sample_size": 153600,
    "sample_rate": 24000,
    "audio_channels": 1,
    "model": {
        "pretransform": {
            "type": "vocos_pretrained",
            "config": {
                "model_type": "charactr/vocos-encodec-24khz",
                "bandwidth": 3.0
            }
        },
        "conditioning": {
            "configs": [
                {
                    "id": "prompt",
                    "type": "phoneme",
                    "config": {
                        "max_length": 128,
                        "project_out": true
                    }
                }
            ],
            "cond_dim": 1024
        },
        "lm": {
            "codebook_pattern": "delay",
            "type": "mamba",
            "config": {
                "embed_dim": 1024,
                "n_layer": 48,
                "fused_add_norm": true,
                "residual_in_fp32": true,
                "rms_norm": true
            }
        }
    },
    "training": {
        "learning_rate": 2e-4,
        "demo": {
            "demo_every": 2500,
            "demo_cond": [
                {"prompt": "Once upon a time."},
                {"prompt": "Each conditioner takes in the relevant conditioning data."}
            ],
            "demo_cfg_scales": [1, 3, 9]
        }
    }
}