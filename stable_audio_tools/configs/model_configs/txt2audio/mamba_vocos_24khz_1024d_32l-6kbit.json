{
    "model_type": "lm",
    "sample_size": 180000,
    "sample_rate": 24000,
    "audio_channels": 1,
    "model": {
        "pretransform": {
            "type": "vocos_pretrained",
            "config": {
                "model_type": "charactr/vocos-encodec-24khz",
                "bandwidth": 3.0
            }
        },
        "conditioning": {
            "configs": [
                {
                    "id": "prompt",
                    "type": "phoneme",
                    "config": {
                        "output_dim": 1024
                    }
                }
            ],
            "cond_dim": 1024
        },
        "lm": {
            "codebook_pattern": "delay",
            "type": "mamba",
            "config": {
                "prepend_cond_dim": 1024,
                "embed_dim": 1024,
                "n_layer": 32,
                "fused_add_norm": true,
                "residual_in_fp32": true,
                "rms_norm": true
            },
            "prepend_cond_ids": ["prompt"]
        }
    },
    "training": {
        "demo": {
            "demo_every": 500,
            "demo_cond": [
                {"prompt": "Once upon a time."},
                {"prompt": "Each conditioner takes in the relevant conditioning data."},
                {"prompt": "How much wood would a woodchuck chuck if a woodchuck could chuck wood?"},
                {"prompt": "The quick brown fox jumps over the lazy dog."},
                {"prompt": "Ugh, no."},
                {"prompt": "I'm sorry, Dave. I'm afraid I can't do that."},
                {"prompt": "What's a fluffy catgirl anyway?"},
                {"prompt": "Explooooooooooosion!"}
            ],
            "demo_cfg_scales": [1, 3, 9]
        },
        "learning_rate": 5e-5,
        "optimizer_configs": {
            "lm": {
                "optimizer": {
                    "type": "AdamW",
                    "config": {
                        "lr": 5e-5,
                        "betas": [0.9, 0.95],
                        "weight_decay": 0.1
                    }
                },
                "scheduler": {
                    "type": "OneCycleLR",
                    "config": {
                        "max_lr": 5e-5,
                        "epochs": 10,
                        "steps_per_epoch": 4677,
                        "pct_start": 0.08,
                        "cycle_momentum": false
                    }
                }
            }
        }
    }
}